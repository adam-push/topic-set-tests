{
  "delay_between_tests_seconds" : 30,
  "test_program_path": "../dotnet-client",
  "command": "dotnet run -- {url} {iterations} {test_type} {unique_values} {topic_value_size} {message_queue_size} {total_topics}",
  "environment_variables": {},
  "parameters": {
    "test_type": [2],
    "iterations": [1000000, 5000000, 10000000, 50000000],
    "unique_values": [1],
    "topic_value_size": [250, 500],
    "message_queue_size": [10000, 100000, 1000000],
    "total_topics": [100, 1000]
  },
  "results": {
    "update_rate": {
      "substring": "Average Update Rate",
      "index": 4
    },
    "average_throughput": {
      "substring": "Average Throughput",
      "index": 3
    },
    "timestamp": {
      "substring": "Test Timestamp",
      "index": 3
    }
  },
  "failed_conditions": [
    "Test Failed",
    "The build failed",
    "SessionClosedException",
    "Connection failed"
  ],
  "log": {
    "path": "~/batch-tester.log"
  },
  "csv": {
    "path": "~/results.csv",
    "sequence": [
      "operating_system",
      "test_type",
      "unique_values",
      "total_topics",
      "iterations",
      "topic_value_size",
      "message_queue_size",
      "update_rate",
      "average_throughput",
      "timestamp",
      "command"
    ]
  }
}